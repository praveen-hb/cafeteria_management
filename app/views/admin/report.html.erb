    FROM:<input class="From-date" type="date" name="from_date" placeholder="From-date">
    TO:<input class="To-date" type="date" name="to_date" placeholder="To-date">
    <%= submit_tag "View all Reports", class: "MenuItems-cart"%>
  <% end %>
  </ul>
  <ul>
    <%= form_with(url: "", method: "",class: "AddOrder") do %>
