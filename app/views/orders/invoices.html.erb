<main class="Order-details">
  <h1 class="Invoice-title"> All Invoice bills </h1>
  <ul class="Menu=items">
  <% @orders.each do |order|%>
    <li class="Menu-item">
      <h2> <%= order.id%> </h2>
      <h2> <%= order.user.name%> </h2>
      <h2> <%= order.value %> </h2>
    </li>
  <% end %>
  </ul>
</main>
